@model IEnumerable<UAS.Core.DAL.Common.Model.Movement>

<div id="attendance" class="row">

    @foreach (var movement in Model)
            {
        <div class="col-lg-3">
            <div class="contact-box">
                <a href="@Url.Action("Profile", "AppViews")">
                    <div class="col-sm-4">
                        <div class="text-center">
                            <img alt="image" class="img-circle m-t-xs img-responsive" src="~/Images/a2.jpg">
                            <div class="m-t-xs font-bold">Graphics designer</div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <h3><strong>@string.Concat(movement.Name, " ", movement.LastName)</strong></h3>
                        <p><i class="fa fa-map-marker"></i> Riviera State 32/106</p>
                        <address>
                            <strong>Twitter, Inc.</strong><br>
                            795 Folsom Ave, Suite 600<br>
                            San Francisco, CA 94107<br>
                            <abbr title="Phone">P:</abbr> (123) 456-7890
                        </address>
                    </div>
                    <div class="clearfix"></div>
                </a>
            </div>
        </div>
    }

</div>