@model IEnumerable<UAS.Core.DAL.Common.Model.StudentMovement>
@{
    ViewBag.Title = "Aula Virtual";
}

<div id="wrapper">
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-sm-4">
            <h2>@ViewBag.Title.ToString().ToUpper()</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="index.html">Asistencia</a>
                </li>
                <li class="active">
                    <strong>@ViewBag.Title</strong>
                </li>
            </ol>
        </div>
    </div>
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row m-b-lg m-t-lg">
            <div class="col-md-6">
                @Html.Partial("CourseDetailPartial")
            </div>
            <div class="col-md-3">
                @Html.Partial("StudentsStatisticPartial")
            </div>
            <div class="col-md-3">
                <small>Comportamiento Asistencia 24h</small>
                <h2 class="no-margins">2 048</h2>
                <div id="sparkline1"></div>
            </div>
        </div>

        @Html.Partial("VirtualStudentsClassRoomPartial", Model)
    </div>

</div>

@section Styles {
    @Styles.Render("~/plugins/toastrStyles")
}

@section Scripts {
    @Scripts.Render("~/plugins/sparkLine")
    @Scripts.Render("~/plugins/toastr")
    <script src="~/Scripts/knockout-3.4.0.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <script src="/signalr/hubs"></script>
    <script src="~/App/AttendanceMovementViewModel.js"></script>
}