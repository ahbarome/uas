@model IEnumerable<UAS.Core.DAL.Common.Model.TeacherMovement>
@{
    ViewBag.Title = "Salón de Docentes";
}

<div id="wrapper">
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-sm-4">
            <h2>@ViewBag.Title.ToString().ToUpper()</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="index.html">Asistencia</a>
                </li>
                <li class="active">
                    <strong>@ViewBag.Title.ToString()</strong>
                </li>
            </ol>
        </div>
    </div>
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row m-b-lg m-t-lg">
            <div class="col-md-5">
                @Html.Partial("DirectorProfilePartial")
            </div>
            <div class="col-md-2">
                @Html.Partial("TeacherAttendanceStatisticPartial")
            </div>
            <div class="col-md-4">
                @Html.Partial("TeacherAttendanceGraphPartial")
            </div>
        </div>
        @Html.Partial("VirtualTeachersClassRoomPartial", Model)
    </div>
</div>
@section Styles {
    @Styles.Render("~/plugins/toastrStyles")
}

@section Scripts {
    @Scripts.Render("~/plugins/sparkLine")
    @Scripts.Render("~/plugins/chartJs")
    @Scripts.Render("~/plugins/toastr")
    <script src="~/Scripts/knockout-3.4.0.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <script src="/signalr/hubs"></script>
    <script src="~/App/TeacherAttendanceMovementViewModel.js"></script>
}
