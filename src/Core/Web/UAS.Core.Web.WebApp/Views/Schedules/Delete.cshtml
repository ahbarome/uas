@model UAS.Core.DAL.Common.Model.Schedule

@{
       ViewBag.Title = "Programación de Cursos";
}

<div id="wrapper">
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-sm-4">
            <h2 class="text-uppercase">@ViewBag.Title</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="#">Administración</a>
                </li>
                <li class="active">
                    <strong>@ViewBag.Title</strong>
                </li>
            </ol>
        </div>
    </div>
    
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Eliminar Programación de Curso</h5>
                    </div>
                    <div class="ibox-content">
    				<h4 class="text-danger">¿Desea eliminar el registro?</h4>
    				 <dl class="dl-horizontal">
            <dt>
                Descripción
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>
    
            <dt>
                Fecha de Registro
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.RegisterDate)
            </dd>
    
            <dt>
                Periodo
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.AcademicPeriod.Id)
            </dd>
    
            <dt>
                Curso
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Course.Name)
            </dd>
    
            <dt>
                Profesor
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Teacher.Name) @Html.DisplayFor(model => model.Teacher.LastName)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Eliminar" class="btn btn-primary" />
                @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-white"})
            </div>
        }
    
    
    
                    </div>
                </div>
            </div>
        </div>
     </div>
</div>