@model UAS.Core.DAL.Common.Model.ScheduleDetail

@{
    ViewBag.Title = "Detalle Programación de Cursos";
}

<div id="wrapper">
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-sm-4">
            <h2 class="text-uppercase">@ViewBag.Title</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="#">Administración</a>
                </li>
                <li class="active">
                    <strong>@ViewBag.Title</strong>
                </li>
            </ol>
        </div>
    </div>
    
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Eliminar Detalle Programación de Curso</h5>
                    </div>
                    <div class="ibox-content">
    				<h4 class="text-danger">Are you sure you want to delete this?</h4>
    				 <dl class="dl-horizontal">
            <dt>
                Día Semana
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.DayOfTheWeek)
            </dd>
    
            <dt>
                Hora Inicio
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.StartTime)
            </dd>
    
            <dt>
                Hora Fin
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.EndTime)
            </dd>
    
            <dt>
                Fecha Registro
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.RegisterDate)
            </dd>
    
            <dt>
                Descripción
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Schedule.Description)
            </dd>
    
            <dt>
                Espacio
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Space.SpaceType.Type) @Html.DisplayFor(model => model.Space.Name)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Eliminar" class="btn btn-primary" />
                @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-white"})
            </div>
        }
    
    
    
                    </div>
                </div>
            </div>
        </div>
     </div>
     </div>